{
  "name": "hangman-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "form-serialize": "^0.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^7.2.6",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jsdom": "12.0.0",
    "jsdom-global": "3.0.2",
    "prettier": "^2.5.1",
    "react-scripts": "^5.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.5.4"
  },
  "scripts": {
    "start": "set PORT=3001 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "plugins": [
      "prettier",
      "react"
    ],
    "extends": [
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
}
